{
  "version": "0.0.0",
  "license": "MIT",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "dev": "tsdx watch",
    "build": "tsdx build",
    "test": "tsdx test --passWithNoTests",
    "lint": "tsdx lint",
    "prepare": "tsdx build",
    "size": "size-limit",
    "analyze": "size-limit --why"
  },
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "publishConfig": {
    "access": "public"
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "name": "@helluwa/database",
  "author": "Karthikeyan Mariappan",
  "description": "Database Abstraction for the Various type of sources.",
  "module": "dist/database.esm.js",
  "size-limit": [
    {
      "path": "dist/database.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/database.esm.js",
      "limit": "10 KB"
    }
  ],
  "devDependencies": {
    "@size-limit/preset-small-lib": "^7.0.4",
    "@upstash/redis": "^1.18.1",
    "husky": "^7.0.4",
    "semantic-release": "^18.0.1",
    "size-limit": "^7.0.4",
    "tsdx": "^0.14.1",
    "tslib": "^2.3.1",
    "typescript": "^4.5.4"
  },
  "repository": {
    "type": "git",
    "url": "template repository url"
  },
  "dependencies": {
    "@helluwa/utils": "*"
  }
}
